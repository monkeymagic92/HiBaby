<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jy.hibaby.message.MessageMapper">
	
	<insert id="insMessage" parameterType="MessagePARAM">
		INSERT INTO t_message
		(ctnt)
		VALUES
		(#{ctnt})
	</insert>
	
	<select id="selMessage" resultType="MessagePARAM">
		SELECT ctnt FROM t_message
	</select>
</mapper>